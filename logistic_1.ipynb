    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "\n",
    "N=100\n",
    "D=2\n",
    "\n",
    "X=np.random.randn(N,D)\n",
    "ones=np.array([[1]*N]).T\n",
    "Xt = np.hstack((ones,X))\n",
    "w=np.random.randn(D+1)\n",
    "z=Xt.dot(w)\n",
    "def stigmoid(Z):\n",
    "    return(1/(1+np.exp(-z)))\n",
    "print(stigmoid(z))"
   ]
